@page "/"

<div style="display: flex">
    @foreach(var media in mediaList)
    {
        @*<img src=@media.path>*@
        Console.WriteLine(media.path);
    }
</div>

@code
{
    public readonly List<Connection.Media> mediaList = LoadMediaTable();

    public static List<Connection.Media> LoadMediaTable()
    {
        Connection c = new Connection();
        return c.LoadMediaTable();
    }
}