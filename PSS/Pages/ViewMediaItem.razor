@*Page for viewing full-sized images in Index*@
@page "/view/{id}"
@using PSS.Backend
@layout EmptyLayout

<ViewItem id="@id" MediaList="@mediaList" Page="view" ReturnPath="/" DateAdded="@C.GetDateAdded(Guid.Parse(id))" IsAlbum="@false"/>

@code{
    [Parameter]
    public string id { get; set; }

    private static readonly List<C.MediaRow> mediaList = C.LoadMediaTable();
}