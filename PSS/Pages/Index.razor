@page "/"
@using PSS.Backend

@foreach (Connection.Media media in mediaList)
{
    <img src=@media.path style="max-width: 10%; padding: 4px" alt="" />
}

@code {
    private readonly List<Connection.Media> mediaList = LoadMediaTable();

    private static List<Connection.Media> LoadMediaTable()
    {
        Connection c = new();
        return c.LoadMediaTable();
    }
}
