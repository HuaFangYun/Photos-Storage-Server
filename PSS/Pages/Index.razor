@page "/"
@using PSS.Backend

<div style="display: flex">
    @foreach (Media media in mediaList)
    {
        <img onclick="location.href='/view/@media.uuid'" id=@media.uuid src=@media.path class="thumbnail" alt="" />
    }
</div>

@code {
    private readonly List<Media> mediaList = Connection.LoadMediaTable();
}
