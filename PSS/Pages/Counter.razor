@page "/counter"
@using PSS.Client.Backend

@foreach (var media in mediaList)
{
    <img src=@media.path style="max-width: 10%" alt=""/>
}

@code {
    public List<Connection.Media> mediaList = LoadMediaTable();

    public static List<Connection.Media> LoadMediaTable()
    {
        Connection c = new Connection();
        return c.LoadMediaTable();
    }
}
