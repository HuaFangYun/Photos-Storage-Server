@*Page for viewing full-sized images in Index*@
@page "/view-no-dt/{id}"
@layout EmptyLayout

<ViewItem id="@id" MediaList="@mediaList" Page="view-no-dt" ReturnPath="/no-dt" DateAdded="@C.GetDateAdded(Guid.Parse(id))"/>

@code{
    [Parameter]
    public string id { get; set; }

    private static readonly List<C.MediaRow> mediaList = C.LoadMediaNoDT();
}